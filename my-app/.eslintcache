 newUserData = {\n            ...instructor,\n            [e.target.name]: e.target.value\n        };\n\n        validateChange(e);\n        setInstructor(newUserData);\n    }\n\n    {/* ------------- YUP SCHEMA -------------------- */}\n\n    const formSchema = yup.object().shape({\n        email: yup\n        .string()\n        .email(\"Must be a valid e-mail address.\")\n        .required(\"Must include email address.\"),\n        username: yup.string().required(\"Username is required.\"),\n        password: yup.string().min(6).max(10).required(\"Must have a valid password between 5 and 10 characters long.\"),\n        role: yup\n        .string()\n        .oneOf([\"Role 1\", \"Role 2\"])\n\n    });\n\n     {/* ------------- USEEFFECT HOOK FOR BUTTON -------------------- */}\n\n     useEffect(() => {\n        console.log('form state change')\n        formSchema.isValid(instructor).then(valid => {\n            console.log('valid?', valid)\n            setButtonDisabled(!valid);\n        });\n\n     }, [instructor]);\n\n\n\n{/* ------------- VALIDATE CHANGE FUNCTION -------------------- */}\n\nconst validateChange = (e) => {\n    yup.reach(formSchema, e.target.name).validate(e.target.value)\n    .then(valid => {\n        setErrors({\n            ...errors, \n            [e.target.name]: \"\"\n        })\n    })\n    .catch(err => {\n        setErrors({\n            ...errors,\n            [e.target.name]: err.errors[0]\n        })\n    })\n}\n\n{/* ------------- FORM SUBMIT FUNCTION -------------------- */}\n{/* ------------- USING DUMMY POST REQUEST -------------------- */}\n\nconst registrationSubmit = e => {\n    e.preventDefault();\n    axios\n    .post(\"https://reqres.in/api/users\", instructor)\n    .then(res => {\n        setPost(res.data);\n        console.log(\"success!\", post);\n        // reset form if succesful\n        setInstructor({\n            email: \"\",\n            username: \"\",\n            password: \"\",\n            role: \"\"\n        })\n    })\n    .catch(err => console.log(err.response));\n}\n\n    return (\n        <div>\n        <div>Instructor Registration</div>\n\n        {/* ------------- INSTRUCTOR FORM -------------------- */}\n        <form onSubmit={registrationSubmit}>\n\n        {/* -------------- E-MAIL -------------- */}\n\n             <label htmlFor=\"email\">e-mail:\n                <input \n                id=\"email\"\n                type=\"email\"\n                name=\"email\"\n                placeholder=\"E-mail here please.\"\n                value={instructor.email}\n                onChange={inputChange}\n                />\n                {errors.email.length > 0 ? <p style={{color: 'red'}}>{errors.email}</p> : null}\n                </label>\n            \n            \n            {/* --------------USERNAME -------------- */}\n            \n            <label htmlFor=\"username\">username:\n            <input\n            id=\"username\"\n            type=\"text\"\n            name=\"username\"\n            placeholder=\"Please enter username here.\"\n            value={instructor.username}\n            onChange={inputChange}\n            />\n            {errors.username.length > 0 ? <p style={{color: 'red'}}>{errors.username}</p> : null}\n            </label>\n\n             {/* -------------- PASSWORD -------------- */}\n\n             <label htmlFor=\"password\">password:\n                <input\n                id=\"password\"\n                type=\"password\"\n                name=\"password\"\n                placeholder=\"Password here, please.\"\n                value={instructor.password}\n                onChange={inputChange}\n                />\n                {errors.password.length > 0 ? <p style={{color: 'red'}}>{errors.password}</p> : null}\n             </label>\n\n              {/* -------------- ROLE -------------- */}\n              <label htmlFor=\"role\">\n                Please select your role.\n                <select \n                id=\"role\" \n                name=\"role\"\n                value={instructor.role}\n                onChange={inputChange}>\n                    <option>---Please choose an option--</option>\n                    <option>Role 1</option>\n                    <option>Role 2</option>\n                </select>\n                {errors.role.length > 0 ? <p style={{color: 'red'}}>{errors.role}</p> : null}\n              </label>\n\n               {/* -------------- SUBMIT BUTTON -------------- */}\n               {/* <pre>{JSON.stringify(post, null, 2)}</pre> */}\n               <button disabled={buttonDisabled}>Ready to register?</button>\n\n        </form>\n        </div>\n    )\n}\n\nexport default InstructorRegister","/Users/calebsmith/Lambda2/bw-unit3/front-end-react-/my-app/src/index.js",[],"/Users/calebsmith/Lambda2/bw-unit3/front-end-react-/my-app/src/Components/HomeComponents/Home.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":2,"column":8,"nodeType":"67","messageId":"68","endLine":2,"endColumn":12},{"ruleId":"65","severity":1,"message":"69","line":37,"column":9,"nodeType":"67","messageId":"68","endLine":37,"endColumn":16},{"ruleId":"70","severity":1,"message":"71","line":74,"column":5,"nodeType":"72","messageId":"73","endLine":74,"endColumn":64},{"ruleId":"70","severity":1,"message":"71","line":78,"column":7,"nodeType":"72","messageId":"73","endLine":78,"endColumn":62},{"ruleId":"70","severity":1,"message":"71","line":85,"column":5,"nodeType":"72","messageId":"73","endLine":85,"endColumn":69},{"ruleId":"70","severity":1,"message":"71","line":98,"column":5,"nodeType":"72","messageId":"73","endLine":98,"endColumn":58},{"ruleId":"70","severity":1,"message":"71","line":105,"column":6,"nodeType":"72","messageId":"73","endLine":105,"endColumn":74},{"ruleId":"74","severity":1,"message":"75","line":114,"column":9,"nodeType":"76","endLine":114,"endColumn":21,"suggestions":"77"},{"ruleId":"70"